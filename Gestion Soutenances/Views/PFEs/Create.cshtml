@model Gestion_Soutenances.Models.PFE

@{
    ViewData["Title"] = "Create";
}

<!-- Add these lines after your existing stylesheet references -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-daterangepicker@3.1.0/daterangepicker.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
<!-- Add these lines after your existing script references -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap-daterangepicker@3.1.0/daterangepicker.js" integrity="sha384-iwHg5FMJ6JSKk+2Z0Fv4IS3p3QUnfT5+dl5BOp7Z2PL1pHHKkOOWt9g1p94ip+7R" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/moment@2.29.1/moment.min.js" integrity="sha384-dDkmdqVqR+1G4jBDY/6zqU5e2KVy1Dp3yof54ksd9nH4l7u2x0e8Z8hD0P4aBoJ" crossorigin="anonymous"></script>


<h1>Ajout d'un PFE</h1>

<h4>PFE</h4>
<hr />
<div class="row">
    <div class="col-md-6">
        <div class="card card-primary">
            <div class="card-header">
                <h3 class="card-title">Ajouter un PFE</h3>
            </div>
            <div class="card-body">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="titre" class="control-label"></label>
                        <input asp-for="titre" class="form-control" />
                        <span asp-validation-for="titre" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="desc" class="control-label"></label>
                        <input asp-for="desc" class="form-control" />
                        <span asp-validation-for="desc" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="DateD" class="control-label"></label>
                        <input asp-for="DateD" class="form-control" />
                        <span asp-validation-for="DateD" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="DateF" class="control-label"></label>
                        <input asp-for="DateF" class="form-control" />
                        <span asp-validation-for="DateF" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="EncadrantId" class="control-label"></label>
                        <select asp-for="EncadrantId" class="form-control" asp-items="ViewBag.EncadrantId"></select>
                    </div>
                    <div class="form-group">
                        <label asp-for="SocieteId" class="control-label"></label>
                        <select asp-for="SocieteId" class="form-control" asp-items="ViewBag.SocieteId"></select>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>
        </div>
    </div>

        <div>
            <a asp-action="Index">Back to List</a>
        </div>

        @section Scripts {
            @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        }
