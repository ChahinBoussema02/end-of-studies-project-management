@model Gestion_Soutenances.Models.Etudiant

@{
    ViewData["Title"] = "Details";
}

<h1 class="text-center">Details</h1>

<div class="container">
    <h4 class="mt-4 mb-3">Etudiant</h4>
    <hr />

    <!-- Default box -->
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">Etudiant</h3>
        </div>
        <div class="card-body">
            <dl class="row">
                <dt class="col-md-2">
                    <strong>@Html.DisplayNameFor(model => model.Nom)</strong>
                </dt>
                <dd class="col-md-10">
                    @Html.DisplayFor(model => model.Nom)
                </dd>

                <dt class="col-md-2 mt-3">
                    <strong>@Html.DisplayNameFor(model => model.Prenom)</strong>
                </dt>
                <dd class="col-md-10">
                    @Html.DisplayFor(model => model.Prenom)
                </dd>

                <dt class="col-md-2 mt-3">
                    <strong>@Html.DisplayNameFor(model => model.DateNaiss)</strong>
                </dt>
                <dd class="col-md-10">
                    @Html.DisplayFor(model => model.DateNaiss)
                </dd>
            </dl>
        </div>

        <div class="card-footer">
            <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.Id">Edit</a>
            <a class="btn btn-secondary" asp-action="Index">Back to List</a>
        </div>
    </div>
</div>
