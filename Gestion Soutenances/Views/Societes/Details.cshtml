@model Gestion_Soutenances.Models.Societe

@{
    ViewData["Title"] = "Details";
}

<h1 class="text-center">Details</h1>

<div class="container">
    <h4 class="mt-4 mb-3">Societe</h4>
    <hr />

    <!-- Default box -->
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">Societe</h3>
        </div>
        <div class="card-body">
            <dl class="row">
                <dt class="col-sm-2">
                    <strong>@Html.DisplayNameFor(model => model.Lib)</strong>
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Lib)
                </dd>

                <dt class="col-sm-2 mt-3">
                    <strong>@Html.DisplayNameFor(model => model.Adresse)</strong>
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Adresse)
                </dd>

                <dt class="col-sm-2 mt-3">
                    <strong>@Html.DisplayNameFor(model => model.Tel)</strong>
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Tel)
                </dd>
            </dl>
        </div>

        <div class="card-footer">
            <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.Id">Edit</a>
            <a class="btn btn-secondary" asp-action="Index">Back to List</a>
        </div>
    </div>
</div>
